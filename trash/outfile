a1=25; b1=25

add() {

        a=`expr $a1 \* $b1`

                return $a

}

add; 
echo $a
#include<iostream>
#include<vector>
using namespace std;


int main(){
	
	
	int  n = 3;
	for(int b = 0 ; b < (1<<n) ; b++){
	
		vector<int> s;
		for(int i = 0 ; i <n ; i++){
			if(b & (1<<i)) cout<<i<<" ";
		}	
		cout<<endl;
		
	}
}
